{
  "publisher": "Trelent",
  "name": "trelent",
  "displayName": "Trelent",
  "description": "We write documentation for you!",
  "version": "1.0.0",
  "repository": "https://github.com/Trelent/Trelent-VSCode-Extension",
  "icon": "images/trelent-icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "trelent.writeDocstrings",
        "title": "Trelent: Write Docstring(s)"
      }
    ],
    "keybindings": [
      {
        "command": "trelent.writeDocstrings",
        "key": "ctrl+shift+2",
        "mac": "cmd+shift+2",
        "when": "editorHasSelection"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorHasSelection",
          "command": "trelent.writeDocstrings"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.18.5",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "tree-sitter-cli": "^0.20.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "axios": "^0.22.0",
    "tree-sitter-java": "^0.19.1",
    "tree-sitter-javascript": "^0.19.0",
    "tree-sitter-python": "^0.19.0",
    "web-tree-sitter": "^0.19.4"
  }
}
